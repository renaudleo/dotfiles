[core]
	excludesfile = ~/.gitignore
[color]
	ui = true
[advice]
	statusHints = false
[alias]
	br = branch
	c = commit -m
	ca = commit -a -m
	co = checkout
	p = push
	pr = pull --rebase
	st = status -bs
	oops = reset --hard HEAD~1
	amend = commit --amend -m
	patch = add --patch
[push]
	default = tracking
